{
  "include": [
    "assets"
  ],
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2018",
    "moduleResolution": "node",
    "jsx": "preserve",
    "allowSyntheticDefaultImports": true,
    "importsNotUsedAsValues": "error",
    "isolatedModules": true,
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "useDefineForClassFields": true,
    "baseUrl": "./",
    "paths": {
      "*": [
        "public/web_modules/.types/*"
      ],
      "@scripts/*": [
        "assets/scripts/*"
      ],
      "@Interfaces/*": [
        "assets/scripts/Interfaces/*"
      ],
      "@Types/*": [
        "assets/scripts/Types/*"
      ],
      "@Enums/*": [
        "assets/scripts/Enums/*"
      ],
      "@Handlers/*": [
        "assets/scripts/Handlers/*"
      ],
      "@Services/*": [
        "assets/scripts/Handlers/*"
      ],
      "@Layers/*": [
        "assets/scripts/Layers/*"
      ],
    },
    "types": [
      "snowpack-env",
      "jest"
    ]
  }
}